                  <a href="tel:0029129102320"> Office Telephone : 0029129102320</a></li>
                <li class="contact__list-item">
                  <a href="tel:000232439493">Mobile : 000 2324 39493</a> </li>
              </ul>
            </div>
          </div>
        </div>
        <div class="contact__navigation">
          <div class="contact__left-nav">
            <form class="contact__form" action="#">
              <input class="contact__name" type="text" placeholder="NAME" required>
              <input class="contact__email" type="email" placeholder="EMAIL" required>
              <textarea class="contact__mess" type="text" placeholder="MESSAGE" required></textarea>
              <button class="contact__btn" type="submit">SEND MESSAGE</button>
            </form>
          </div>
          <div class="contact__right-nav">
          <div id="map"></div>

          </div>
        </div>
      </div>
    </section>


    

